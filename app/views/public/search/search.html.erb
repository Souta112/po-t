<h2>検索結果</h2>

<table class="table table-hover table-inverse">
  <% if @range == "1" %>
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td><%= user.name %></td>
        </tr>
      <% end %>
    </tbody>
  <% else %>
    <!-- -->
    <tbody>
      <% @memos.each do |memo| %>
        <tr>
          <td><%= memo.title %></td>
          <td><%= memo.text %></td>
        </tr>
      <% end %>
    </tbody>
  <% end %>
</table>
