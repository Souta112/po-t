<h2>Results index</h2>

<table class="table table-hover table-inverse">
  <% if @range == "1" %>
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td><%= user.name %></td>
        </tr>
        <tr>
        <td><%= link_to "Destroy",admin_user_path(user.id), method: :delete, class: "btn btn-sm btn-danger" %></td>
        <tr/>
      <% end %>
    </tbody>
  <% else %>
    <!-- -->
    <tbody>
      <% @memos.each do |memo| %>
        <tr>
          <td><%= memo.title %></td>
          <td><%= memo.text %></td>
        </tr>
      <% end %>
    </tbody>
  <% end %>
</table>